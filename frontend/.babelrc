{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        "transform-react-jsx",
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }],
        ["import", {
            "libraryName": "antd",
            "style": true,
        }]
    ]
}
